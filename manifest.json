{
	"name": "Typeracer premium UI",
	"version": "0.1.0",
	"description": "Changes UI to look like premium account",
	"content_scripts": [
		{
			"matches": ["*://play.typeracer.com/*"],
			"js": ["javascript/general.js", "javascript/typeracer.js", "javascript/contentScript.js"]
		}
	],
	"permissions": ["storage", "tabs"],
	"browser_action": {
		"default_icon": "images/icon.png",
		"default_popup": "popup/popup.html"
	},
	"manifest_version": 2
}